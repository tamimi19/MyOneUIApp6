<?xml version="1.0" encoding="utf-8"?>
<de.dlyt.yanndroid.oneui.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:drawer_viewId="@id/viewInDrawer"
    app:toolbar_title="@string/app_name"
    app:toolbar_subtitle="@string/app_subtitle"
    app:toolbar_expanded="true"
    app:toolbar_expandable="true">

    <include
        android:id="@+id/viewInDrawer"
        layout="@layout/content_drawer" />

    <include layout="@layout/content_main" />

</de.dlyt.yanndroid.oneui.widget.DrawerLayout>
